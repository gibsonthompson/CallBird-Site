{
  "rewrites": [
    { "source": "/blog", "destination": "/blog.html" },
    { "source": "/blog/ai-receptionist-guide", "destination": "/blog-ai-receptionist-guide.html" },
    { "source": "/blog-ai-receptionist-guide", "destination": "/blog-ai-receptionist-guide.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
